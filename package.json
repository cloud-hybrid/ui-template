{
    "name": "@cloud-technology/ui-template",
    "version": "0.0.0",
    "private": true,
    "main": "./src/index.js",
    "scripts": {
        "setup": "node .ci",
        "sass": "sass --watch ./src --load-path ./node_modules/@carbon --load-path node_modules --load-path .",
        "clean-ts": "find . -type f -name \"*.d.ts\" -delete",
        "clean-css": "find . -type f -name \"*.css\" -delete && find . -type f -name \"*.css.map\" -delete",
        "analyze": "npm run build --production && source-map-explorer \"build/static/js/*.js\"",
        "build": "react-scripts build",
        "eject": "react-scripts eject",
        "start": "react-scripts start",
        "test": "react-scripts test"
    },
    "dependencies": {
        "@carbon/elements": "^10.45.0",
        "@carbon/feature-flags": "^0.6.0",
        "@carbon/grid": "^10.38.0",
        "@carbon/ibm-cloud-cognitive": "^0.93.1",
        "@carbon/icons-react": "^10.41.0",
        "@carbon/react": "^0.7.0",
        "axios": "^0.21.4",
        "axios-cache-adapter": "^2.7.3",
        "browserify": "latest",
        "carbon-components": "^10.46.0",
        "carbon-components-react": "^7.46.0",
        "localforage": "^1.10.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-router-dom": "^5.3.0",
        "react-scripts": "^4.0.3"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^*",
        "@testing-library/react": "^*",
        "@testing-library/user-event": "^*",
        "@types/browserify": "^*",
        "@types/cacheable-request": "^*",
        "@types/carbon__colors": "^*",
        "@types/carbon__elements": "^*",
        "@types/carbon__feature-flags": "^*",
        "@types/carbon__icon-helpers": "^*",
        "@types/carbon__icons-react": "^*",
        "@types/carbon__type": "^*",
        "@types/carbon-components": "^*",
        "@types/carbon-components-react": "^*",
        "@types/core-js": "^*",
        "@types/debug": "^*",
        "@types/eslint-scope": "^*",
        "@types/express": "^*",
        "@types/jest": "^*",
        "@types/jsonwebtoken": "^*",
        "@types/lodash": "^*",
        "@types/node": "^16.11.6",
        "@types/react": "^*",
        "@types/react-dom": "^*",
        "@types/react-router-dom": "^*",
        "@types/resize-observer-browser": "^*",
        "html-webpack-plugin": "^5.5.0",
        "postcss-normalize": "^10.0.1",
        "prop-types": "^15.7.2",
        "sass": "1.38.2",
        "serve": "^12.0.1",
        "source-map-explorer": "^2.5.2",
        "web-vitals": "^2.1.0",
        "webpack": "^5.60.0",
        "webpack-cli": "^4.9.1"
    },
    "lint-staged": {
        "*.js": "eslint --cache --fix",
        "*.{js,md,yml}": "prettier --write"
    },
    "browserslist": [
        "last 1 version"
    ],
    "homepage": "/"
}
